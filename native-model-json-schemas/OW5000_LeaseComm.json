      "sqlType": "DBAPITYPEOBJECT('CHAR', 'NCHAR', 'NVARCHAR', 'VARCHAR', 'OTHER')"
    },
    "commitment_type": {
      "type": "string",
      "maxLength": 12,
      "sqlType": "DBAPITYPEOBJECT('CHAR', 'NCHAR', 'NVARCHAR', 'VARCHAR', 'OTHER')"
    },
    "start_date": {
      "type": "string",
      "format": "time",
      "maxLength": 29,
      "sqlType": "DBAPITYPEOBJECT('TIMESTAMP')"
    },
    "planned_completion": {
      "type": "string",
      "format": "time",
      "maxLength": 29,
      "sqlType": "DBAPITYPEOBJECT('TIMESTAMP')"
    },
    "actual_completion": {
      "type": "string",
      "format": "time",
      "maxLength": 29,
      "sqlType": "DBAPITYPEOBJECT('TIMESTAMP')"
    },
    "completed_ind": {
      "type": "string",
      "maxLength": 1,
      "sqlType": "DBAPITYPEOBJECT('CHAR', 'NCHAR', 'NVARCHAR', 'VARCHAR', 'OTHER')"
    },
    "amount": {
      "type": "number",
      "format": "float",
      "multipleOf": 0.001,
      "sqlType": "DBAPITYPEOBJECT('FLOAT', 'REAL', 'DOUBLE')"
    },
    "currency": {
      "type": "string",
      "maxLength": 24,
      "sqlType": "DBAPITYPEOBJECT('CHAR', 'NCHAR', 'NVARCHAR', 'VARCHAR', 'OTHER')"
    },
    "remark": {
      "type": "string",
      "maxLength": 2000,
      "sqlType": "DBAPITYPEOBJECT('CHAR', 'NCHAR', 'NVARCHAR', 'VARCHAR', 'OTHER')"
    },
