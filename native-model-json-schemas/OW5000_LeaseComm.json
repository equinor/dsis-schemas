      "type": "string",
      "format": "time",
      "maxLength": 29,
      "sqlType": "DBAPITYPEOBJECT('TIMESTAMP')"
    },
    "update_date": {
      "type": "string",
      "format": "time",
      "maxLength": 29,
      "sqlType": "DBAPITYPEOBJECT('TIMESTAMP')"
    },
    "create_user_id": {
      "type": "string",
      "maxLength": 32,
      "sqlType": "DBAPITYPEOBJECT('CHAR', 'NCHAR', 'NVARCHAR', 'VARCHAR', 'OTHER')"
