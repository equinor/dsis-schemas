{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "#/definitions/OW5000_ProjectExtension",
  "title": "OW5000.ProjectExtension",
  "type": "object",
  "properties": {
    "project_name": {
      "type": "string",
      "maxLength": 20,
      "sqlType": "DBAPITYPEOBJECT('CHAR', 'NCHAR', 'NVARCHAR', 'VARCHAR', 'OTHER')"
    },
    "release_id": {
      "type": "string",
      "maxLength": 32,
      "sqlType": "DBAPITYPEOBJECT('CHAR', 'NCHAR', 'NVARCHAR', 'VARCHAR', 'OTHER')"
    },
    "extension_name": {
      "type": "string",
      "maxLength": 40,
      "sqlType": "DBAPITYPEOBJECT('CHAR', 'NCHAR', 'NVARCHAR', 'VARCHAR', 'OTHER')"
    },
    "extension_date": {
      "type": "string",
      "format": "time",
      "maxLength": 29,
      "sqlType": "DBAPITYPEOBJECT('TIMESTAMP')"
    },
    "extension_user_id": {
      "type": "string",
      "maxLength": 32,
      "sqlType": "DBAPITYPEOBJECT('CHAR', 'NCHAR', 'NVARCHAR', 'VARCHAR', 'OTHER')"
    },
    "remark": {
      "type": "string",
      "maxLength": 2000,
      "sqlType": "DBAPITYPEOBJECT('CHAR', 'NCHAR', 'NVARCHAR', 'VARCHAR', 'OTHER')"
    },
    "native_uid": {
      "type": "string",
      "maxLength": 4000,
      "sqlType": "DBAPITYPEOBJECT('CHAR', 'NCHAR', 'NVARCHAR', 'VARCHAR', 'OTHER')"
    }
  },
  "required": [
    "project_name",
    "release_id",
    "extension_name"
  ]
}